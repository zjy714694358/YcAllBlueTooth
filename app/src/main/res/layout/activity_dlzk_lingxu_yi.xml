<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/zhuti2"
    tools:context=".dlzk.activity.DlzkLingxuYiActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center_vertical"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="30dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/canshushezhi"
            android:theme="@style/textSize_25_b" />

        <TextView
            android:id="@+id/tvDlzkLingxuYiTime"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:theme="@style/textSize_20_b"
            android:text=""/>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/white"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:gravity="center"
        android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:theme="@style/textSize_20"
            android:text="@string/ceshicanshu"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="15dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:theme="@style/textSize_18"
                android:background="@drawable/bac_ju_yinying_bai"
                android:text="@string/edingrongliang"/>
            <EditText
                android:id="@+id/etDlzkLingxuYiEdrl"
                android:layout_width="100dp"
                android:layout_height="28dp"
                android:layout_marginLeft="5dp"
                android:paddingLeft="5dp"
                android:background="@drawable/et_bac_bai_yinying_hei"
                android:theme="@style/textSize_18_b"
                android:inputType="numberDecimal"
                android:imeOptions="actionDone"
                android:text="30.000"/>
            <TextView
                android:layout_width="50dp"
                android:layout_height="28dp"
                android:theme="@style/textSize_22"
                android:layout_marginLeft="5dp"
                android:text="KVA"/>
            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:theme="@style/textSize_18"
                android:layout_marginLeft="15dp"
                android:background="@drawable/bac_ju_yinying_bai"
                android:text="@string/xuanzedianyuan"/>
            <TextView
                android:id="@+id/tvDlzkLingxuYiXzdy"
                android:layout_width="100dp"
                android:layout_height="28dp"
                android:layout_marginLeft="5dp"
                android:gravity="center_vertical"
                android:background="@drawable/et_bac_bai_yinying_hei"
                android:paddingLeft="5dp"
                android:theme="@style/textSize_18_b"
                android:text="@string/neibu"/>
            <LinearLayout
                android:layout_width="28dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_marginLeft="5dp"
                android:orientation="horizontal">
                <LinearLayout
                    android:id="@+id/llDlzkLingxuYiXzdy"
                    android:layout_width="match_parent"
                    android:layout_height="28dp"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:background="@drawable/btn_lv_yinying_hei">
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/jiantou_down"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="15dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:theme="@style/textSize_18"
                android:background="@drawable/bac_ju_yinying_bai"
                android:text="@string/fenjiedianya"/>
            <EditText
                android:id="@+id/etDlzkLingxuYiFjdy"
                android:layout_width="100dp"
                android:layout_height="28dp"
                android:layout_marginLeft="5dp"
                android:paddingLeft="5dp"
                android:background="@drawable/et_bac_bai_yinying_hei"
                android:theme="@style/textSize_18_b"
                android:inputType="numberDecimal"
                android:imeOptions="actionDone"
                android:text="10.000"/>
            <TextView
                android:layout_width="50dp"
                android:layout_height="28dp"
                android:theme="@style/textSize_22"
                android:layout_marginLeft="5dp"
                android:text="KV"/>
            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:theme="@style/textSize_18"
                android:layout_marginLeft="15dp"
                android:background="@drawable/bac_ju_yinying_bai"
                android:text="@string/celiangwendu"/>
            <EditText
                android:id="@+id/etDlzkLingxuYiClwd"
                android:layout_width="100dp"
                android:layout_height="28dp"
                android:layout_marginLeft="5dp"
                android:paddingLeft="5dp"
                android:background="@drawable/et_bac_bai_yinying_hei"
                android:theme="@style/textSize_18_b"
                android:inputType="numberDecimal"
                android:imeOptions="actionDone"
                android:text="19.00"/>
            <TextView
                android:layout_width="28dp"
                android:layout_height="28dp"
                android:layout_marginLeft="5dp"
                android:gravity="center_vertical"
                android:theme="@style/textSize_18_b"
                android:text="â„ƒ"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="15dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:theme="@style/textSize_18"
                android:background="@drawable/bac_ju_yinying_bai"
                android:text="@string/mingpaizukang"/>
            <EditText
                android:id="@+id/etDlzkLingxuYiMpzk"
                android:layout_width="100dp"
                android:layout_height="28dp"
                android:layout_marginLeft="5dp"
                android:paddingLeft="5dp"
                android:background="@drawable/et_bac_bai_yinying_hei"
                android:theme="@style/textSize_18_b"
                android:inputType="numberDecimal"
                android:imeOptions="actionDone"
                android:text="4.010"/>
            <TextView
                android:layout_width="50dp"
                android:layout_height="28dp"
                android:theme="@style/textSize_22"
                android:layout_marginLeft="5dp"
                android:text="%"/>
            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:visibility="invisible"
                android:theme="@style/textSize_18"
                android:layout_marginLeft="15dp"
                android:background="@drawable/bac_ju_yinying_bai"
                android:text="@string/xuanzedianyuan"/>
            <TextView
                android:layout_width="100dp"
                android:layout_height="28dp"
                android:layout_marginLeft="5dp"
                android:gravity="center_vertical"
                android:visibility="invisible"
                android:background="@drawable/et_bac_bai_yinying_hei"
                android:paddingLeft="5dp"
                android:theme="@style/textSize_18_b"
                android:text="@string/neibu"/>
            <LinearLayout
                android:layout_width="28dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_marginLeft="5dp"
                android:visibility="invisible"
                android:orientation="horizontal">
                <LinearLayout
                    android:id="@+id/llDlzkSanxiangYiXzdy"
                    android:layout_width="match_parent"
                    android:layout_height="28dp"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:background="@drawable/btn_lv_yinying_hei">
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/jiantou_down"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_marginTop="40dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:theme="@style/textSize_18"
                android:background="@drawable/bac_ju_yinying_bai"
                android:text="@string/shipinbianhao"/>
            <EditText
                android:id="@+id/etDlzkLingxuYiSpbh"
                android:layout_width="130dp"
                android:layout_height="28dp"
                android:layout_marginLeft="5dp"
                android:paddingLeft="5dp"
                android:background="@drawable/et_bac_bai_yinying_hei"
                android:theme="@style/textSize_18_b"
                android:imeOptions="actionDone"
                android:text=""/>

            <TextView
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:theme="@style/textSize_18"
                android:layout_marginLeft="40dp"
                android:background="@drawable/bac_ju_yinying_bai"
                android:text="@string/ceshirenyuan"/>
            <EditText
                android:id="@+id/etDlzkLingxuYiCsry"
                android:layout_width="130dp"
                android:layout_height="28dp"
                android:layout_marginLeft="5dp"
                android:paddingLeft="5dp"
                android:background="@drawable/et_bac_bai_yinying_hei"
                android:theme="@style/textSize_18_b"
                android:imeOptions="actionDone"
                android:text=""/>

        </LinearLayout>

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/white"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:gravity="center_vertical"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="30dp"
        android:orientation="horizontal">
        <View
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"/>
        <LinearLayout
            android:id="@+id/llDlzkLingxuYiCeshi"
            android:layout_width="wrap_content"
            android:layout_height="35dp"

            android:gravity="center"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="35dp"
                android:theme="@style/textSize_20_b"
                android:gravity="center"
                android:background="@drawable/btn_lv_yinying_hei"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:text="@string/ceshi"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/llDlzkLingxuYiFanhui"
            android:layout_width="wrap_content"
            android:layout_height="35dp"

            android:gravity="center"
            android:layout_marginLeft="200dp"
            android:layout_marginRight="30dp"
            android:orientation="vertical">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="33dp"
                android:theme="@style/textSize_20_b"
                android:background="@drawable/btn_lv_yinying_hei"
                android:gravity="center"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:text="@string/fanhui"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>